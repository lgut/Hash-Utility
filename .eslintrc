{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "import/prefer-default-export":"off",
    "linebreak-style": 0,
    "quotes":"off",
    "semi":"off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
